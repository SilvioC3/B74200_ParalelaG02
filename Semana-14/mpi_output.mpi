#SBATCH --ntasks 8		# Total number of mpi tasks requested
#SBATCH -t 00:30:00		# Run time (hh:mm:ss) - 30 min
#SBATCH -p debug		# Partition to use, can be: debug, normal

prun ./mpi_output.out

